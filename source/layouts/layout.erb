<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <title>HackCentral API Docs</title>

    <!-- Import CSS/JS assets -->
    <%= stylesheet_link_tag "normalize" %>
    <%= stylesheet_link_tag "bootstrap-3.3.2.min" %>
    <%= javascript_include_tag  "jquery-2.1.3.min" %>
    <%= javascript_include_tag  "bootstrap-3.3.2.min" %>

  </head>

  <body class="<%= page_classes %>">
    <div class="container">
      <div class="row">

        <br>
        <br>

        <div class="col-md-8">
          <%= yield %>

          <%= partial "partials/oauth_authorize" %>
        </div><!-- .col-md-8 -->

        <div class="col-md-4">
          <br>

          <div class="well">
            <%= partial "partials/nav" %>
          </div>
        </div><!-- .col-md-4 -->

      </div><!-- .row -->
    </div><!-- .container -->
  </body>
</html>
